#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/ganache-core@2.13.0/node_modules/ganache-core/node_modules/node-gyp-build/bin.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ganache-core@2.13.0/node_modules/ganache-core/node_modules/node-gyp-build/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ganache-core@2.13.0/node_modules/ganache-core/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ganache-core@2.13.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/bin.js" "$@"
else 
  exec node  "$basedir/../node-gyp-build/bin.js" "$@"
fi
