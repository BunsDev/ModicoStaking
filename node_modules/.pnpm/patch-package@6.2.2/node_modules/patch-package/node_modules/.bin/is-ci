#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/is-ci@2.0.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/patch-package@6.2.2/node_modules/patch-package/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/patch-package@6.2.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
fi
