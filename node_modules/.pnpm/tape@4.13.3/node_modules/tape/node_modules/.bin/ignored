#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/bin/ignored/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/dotignore@0.1.2/node_modules/dotignore/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/dotignore@0.1.2/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/tape@4.13.3/node_modules/tape/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/tape@4.13.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../dotignore@0.1.2/node_modules/dotignore/bin/ignored" "$@"
else 
  exec node  "$basedir/../../../../../dotignore@0.1.2/node_modules/dotignore/bin/ignored" "$@"
fi
