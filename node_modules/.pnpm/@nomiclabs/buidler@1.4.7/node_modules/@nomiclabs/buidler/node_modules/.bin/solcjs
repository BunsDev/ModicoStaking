#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/solc@0.6.8/node_modules/solc/solcjs/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/solc@0.6.8/node_modules/solc/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/solc@0.6.8/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@nomiclabs/buidler@1.4.7/node_modules/@nomiclabs/buidler/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@nomiclabs/buidler@1.4.7/node_modules/@nomiclabs/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@nomiclabs/buidler@1.4.7/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@nomiclabs/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../solc@0.6.8/node_modules/solc/solcjs" "$@"
else 
  exec node  "$basedir/../../../../../../../solc@0.6.8/node_modules/solc/solcjs" "$@"
fi
