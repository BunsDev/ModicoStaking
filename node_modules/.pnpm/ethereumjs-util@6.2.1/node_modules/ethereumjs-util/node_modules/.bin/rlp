#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/bin/rlp/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/rlp@2.2.6/node_modules/rlp/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/rlp@2.2.6/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereumjs-util@6.2.1/node_modules/ethereumjs-util/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereumjs-util@6.2.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rlp@2.2.6/node_modules/rlp/bin/rlp" "$@"
else 
  exec node  "$basedir/../../../../../rlp@2.2.6/node_modules/rlp/bin/rlp" "$@"
fi
