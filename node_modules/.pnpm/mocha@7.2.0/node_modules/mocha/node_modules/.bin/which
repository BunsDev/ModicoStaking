#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/which/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/which@1.3.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/mocha@7.2.0/node_modules/mocha/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/mocha@7.2.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
else 
  exec node  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
fi
