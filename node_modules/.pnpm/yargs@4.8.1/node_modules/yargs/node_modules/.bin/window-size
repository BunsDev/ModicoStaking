#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/window-size@0.2.0/node_modules/window-size/cli.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/window-size@0.2.0/node_modules/window-size/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/window-size@0.2.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/yargs@4.8.1/node_modules/yargs/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/yargs@4.8.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../window-size@0.2.0/node_modules/window-size/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../window-size@0.2.0/node_modules/window-size/cli.js" "$@"
fi
