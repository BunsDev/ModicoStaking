#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/miller-rabin@4.0.1/node_modules/miller-rabin/bin/miller-rabin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/miller-rabin@4.0.1/node_modules/miller-rabin/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/miller-rabin@4.0.1/node_modules/miller-rabin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/miller-rabin@4.0.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethashjs@0.0.8/node_modules/ethashjs/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethashjs@0.0.8/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../miller-rabin@4.0.1/node_modules/miller-rabin/bin/miller-rabin" "$@"
else 
  exec node  "$basedir/../../../../../miller-rabin@4.0.1/node_modules/miller-rabin/bin/miller-rabin" "$@"
fi
