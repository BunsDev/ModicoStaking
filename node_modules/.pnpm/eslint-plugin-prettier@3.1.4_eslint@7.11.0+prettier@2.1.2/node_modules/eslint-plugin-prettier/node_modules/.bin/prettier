#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/prettier@2.1.2/node_modules/prettier/bin-prettier.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/prettier@2.1.2/node_modules/prettier/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/prettier@2.1.2/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/eslint-plugin-prettier@3.1.4_eslint@7.11.0+prettier@2.1.2/node_modules/eslint-plugin-prettier/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/eslint-plugin-prettier@3.1.4_eslint@7.11.0+prettier@2.1.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.1.2/node_modules/prettier/bin-prettier.js" "$@"
else 
  exec node  "$basedir/../../../../../prettier@2.1.2/node_modules/prettier/bin-prettier.js" "$@"
fi
