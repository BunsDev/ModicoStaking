#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/patch-package@6.2.2/node_modules/patch-package/index.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/patch-package@6.2.2/node_modules/patch-package/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/patch-package@6.2.2/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@ethereum-waffle/provider@3.1.1/node_modules/@ethereum-waffle/provider/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@ethereum-waffle/provider@3.1.1/node_modules/@ethereum-waffle/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@ethereum-waffle/provider@3.1.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/@ethereum-waffle/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../patch-package@6.2.2/node_modules/patch-package/index.js" "$@"
else 
  exec node  "$basedir/../../../../../../../patch-package@6.2.2/node_modules/patch-package/index.js" "$@"
fi
