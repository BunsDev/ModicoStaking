#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/opencollective-postinstall@2.0.3/node_modules/opencollective-postinstall/index.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/opencollective-postinstall@2.0.3/node_modules/opencollective-postinstall/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/opencollective-postinstall@2.0.3/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules/husky/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../opencollective-postinstall@2.0.3/node_modules/opencollective-postinstall/index.js" "$@"
else 
  exec node  "$basedir/../../../../../opencollective-postinstall@2.0.3/node_modules/opencollective-postinstall/index.js" "$@"
fi
