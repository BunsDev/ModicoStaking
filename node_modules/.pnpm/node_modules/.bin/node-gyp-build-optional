#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/node-gyp-build@4.2.3/node_modules/node-gyp-build/optional.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node-gyp-build@4.2.3/node_modules/node-gyp-build/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node-gyp-build@4.2.3/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/node-gyp-build/optional.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/node-gyp-build/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/optional.js" "$@"
else 
  exec node  "$basedir/../node-gyp-build/optional.js" "$@"
fi
