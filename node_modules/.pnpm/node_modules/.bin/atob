#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/atob.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/atob@2.1.2/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/atob/bin/atob.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/atob/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/atob/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../atob/bin/atob.js" "$@"
else 
  exec node  "$basedir/../atob/bin/atob.js" "$@"
fi
