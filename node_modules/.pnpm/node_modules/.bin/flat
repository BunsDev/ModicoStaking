#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/flat@4.1.0/node_modules/flat/cli.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/flat@4.1.0/node_modules/flat/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/flat@4.1.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/flat/cli.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/flat/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../flat/cli.js" "$@"
else 
  exec node  "$basedir/../flat/cli.js" "$@"
fi
