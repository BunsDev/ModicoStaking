#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/bin.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/rimraf@2.7.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/rimraf/bin.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/rimraf/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else 
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
