#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/tape@4.13.3/node_modules/tape/bin/tape/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/tape@4.13.3/node_modules/tape/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/tape@4.13.3/node_modules/tape/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/tape@4.13.3/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/tape/bin/tape/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/tape/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/tape/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tape/bin/tape" "$@"
else 
  exec node  "$basedir/../tape/bin/tape" "$@"
fi
