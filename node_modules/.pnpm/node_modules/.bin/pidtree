#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/bin/pidtree.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/pidtree@0.3.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/pidtree/bin/pidtree.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/pidtree/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules/pidtree/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pidtree/bin/pidtree.js" "$@"
else 
  exec node  "$basedir/../pidtree/bin/pidtree.js" "$@"
fi
