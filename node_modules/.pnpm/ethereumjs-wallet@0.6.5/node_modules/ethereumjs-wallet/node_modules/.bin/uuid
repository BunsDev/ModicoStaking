#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/uuid@3.4.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereumjs-wallet@0.6.5/node_modules/ethereumjs-wallet/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereumjs-wallet@0.6.5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
fi
