#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/solhint@3.2.1/node_modules/solhint/solhint.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/solhint@3.2.1/node_modules/solhint/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/solhint@3.2.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/solhint/solhint.js/node_modules:/Users/unico/LoyaltyMine/node_modules/solhint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solhint/solhint.js" "$@"
else 
  exec node  "$basedir/../solhint/solhint.js" "$@"
fi
