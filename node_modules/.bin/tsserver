#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/typescript@4.0.3/node_modules/typescript/bin/tsserver/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/typescript@4.0.3/node_modules/typescript/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/typescript@4.0.3/node_modules/typescript/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/typescript@4.0.3/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/typescript/bin/tsserver/node_modules:/Users/unico/LoyaltyMine/node_modules/typescript/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/typescript/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
