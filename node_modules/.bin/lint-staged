#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/lint-staged@10.4.0/node_modules/lint-staged/bin/lint-staged.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/lint-staged@10.4.0/node_modules/lint-staged/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/lint-staged@10.4.0/node_modules/lint-staged/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/lint-staged@10.4.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/lint-staged/bin/lint-staged.js/node_modules:/Users/unico/LoyaltyMine/node_modules/lint-staged/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/lint-staged/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else 
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
