#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/shx@0.3.2/node_modules/shx/lib/cli.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/shx@0.3.2/node_modules/shx/lib/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/shx@0.3.2/node_modules/shx/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/shx@0.3.2/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/shx/lib/cli.js/node_modules:/Users/unico/LoyaltyMine/node_modules/shx/lib/node_modules:/Users/unico/LoyaltyMine/node_modules/shx/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shx/lib/cli.js" "$@"
else 
  exec node  "$basedir/../shx/lib/cli.js" "$@"
fi
