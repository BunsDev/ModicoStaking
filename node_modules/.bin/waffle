#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereum-waffle@3.1.1/node_modules/ethereum-waffle/bin/waffle/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereum-waffle@3.1.1/node_modules/ethereum-waffle/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereum-waffle@3.1.1/node_modules/ethereum-waffle/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/ethereum-waffle@3.1.1/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/ethereum-waffle/bin/waffle/node_modules:/Users/unico/LoyaltyMine/node_modules/ethereum-waffle/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/ethereum-waffle/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ethereum-waffle/bin/waffle" "$@"
else 
  exec node  "$basedir/../ethereum-waffle/bin/waffle" "$@"
fi
