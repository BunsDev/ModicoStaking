#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/mocha@8.1.3/node_modules/mocha/bin/_mocha/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/mocha@8.1.3/node_modules/mocha/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/mocha@8.1.3/node_modules/mocha/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/mocha@8.1.3/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/mocha/bin/_mocha/node_modules:/Users/unico/LoyaltyMine/node_modules/mocha/bin/node_modules:/Users/unico/LoyaltyMine/node_modules/mocha/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/_mocha" "$@"
else 
  exec node  "$basedir/../mocha/bin/_mocha" "$@"
fi
