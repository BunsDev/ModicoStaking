#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules/husky/lib/upgrader/bin.js/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules/husky/lib/upgrader/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules/husky/lib/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules/husky/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/husky@4.3.0/node_modules:/Users/unico/LoyaltyMine/node_modules/.pnpm/node_modules:/Users/unico/LoyaltyMine/node_modules:/Users/unico/node_modules:/Users/node_modules:/node_modules:/Users/unico/LoyaltyMine/node_modules/husky/lib/upgrader/bin.js/node_modules:/Users/unico/LoyaltyMine/node_modules/husky/lib/upgrader/node_modules:/Users/unico/LoyaltyMine/node_modules/husky/lib/node_modules:/Users/unico/LoyaltyMine/node_modules/husky/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/lib/upgrader/bin.js" "$@"
else 
  exec node  "$basedir/../husky/lib/upgrader/bin.js" "$@"
fi
